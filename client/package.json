    "react": "18.0.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.69.5",
    "react-native-background-timer": "^2.3.1",
    "react-native-circular-progress": "^1.3.7",
    "react-native-code-push": "^7.0.5",
    "react-native-device-info": "^10.0.2",
